{
    "name": "ksyukaksyu/diploma",
    "description": "Netology Diploma Project",
    "keywords": ["netology"],
    "homepage": "https://www.github.com/ksyukaksyu/netologyDiploma/",
    "type": "project",
    "license": "MIT",
    "support": {
        "source": "https://www.github.com/ksyukaksyu/netologyDiploma/"
    },
    "minimum-stability": "stable",
    "prefer-stable" : true,
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-twig": "~2.0.0",
        "macgyer/yii2-materializecss": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "aayaresko/yii2-extended-gii": "~2.0.5@dev"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
